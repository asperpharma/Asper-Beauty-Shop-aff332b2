# Supabase Configuration
# Get these from your Supabase project settings: https://supabase.com/dashboard/project/[PROJECT_ID]/settings/api
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-key"
VITE_SUPABASE_URL="https://your-project-id.supabase.co"

# Shopify Configuration (client-side public token)
# Get these from your Shopify admin: Apps > Headless > Storefront API
VITE_SHOPIFY_STORE_DOMAIN="your-store.myshopify.com"
VITE_SHOPIFY_STOREFRONT_TOKEN="your-storefront-access-token"
VITE_SHOPIFY_API_VERSION="2025-07"

# Site Configuration (optional - for production)
VITE_SITE_URL="https://www.asperbeautyshop.com"
VITE_LOVABLE_URL="https://asperbeautyshop.lovable.app"
